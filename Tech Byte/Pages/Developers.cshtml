<style>
    body {
        background-color: whitesmoke;
    }

    .team-members {
        display: flex;
        justify-content: space-around;
        gap: 30px;
        flex-wrap: wrap;
    }

    .team-member {
        text-align: center;
        background-color: #fff;
        border-radius: 8px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        padding: 20px;
        width: 250px;
    }

    .team-member img {
        width: 100%;
        height: auto;
        border-radius: 50%;
        margin-bottom: 20px;
    }

    h3 {
        font-size: 1.5rem;
        margin-bottom: 10px;
    }

    p {
        font-size: 1rem;
        color: #666;
        margin-bottom: 20px;
    }

    .social-links {
        display: flex;
        justify-content: center;
        gap: 10px;
    }

    .social-links a {
        font-size: 1.5rem;
        color: #333;
        text-decoration: none;
    }

    .social-links a:hover {
        color: #007bff;
    }

    .custom-tooltip .tooltip-inner {
        background-color: black !important;
        color: white !important;
        font-weight: 500;
        padding: 5px 10px;
        border-radius: 4px;
    }

    .custom-tooltip.bs-tooltip-top .tooltip-arrow::before {
        border-top-color: black !important;
    }
</style>

